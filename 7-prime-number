public Boolean isPrime(Integer num) {
    //code here

    boolean retStat;
    if(num != 2)
    {
        for(integer i = 2 ; i < num ; i++)
    {
        if(math.mod(num,i) == 0)
        {
            retStat = false;
            break;
        }else{
            retStat = true;
        }
    }
    }else{
        retStat = true;
    }
    

    return retStat;
}